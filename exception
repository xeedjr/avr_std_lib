#ifndef EXCEPTION_
#define EXCEPTION_

#include "throw.h"

namespace std {

class exception {
public:
  exception () noexcept;
  exception (const exception&) noexcept;
  exception& operator= (const exception&) noexcept;
  virtual ~exception();
  virtual const char* what() const noexcept;
};

};

#endif /* EXCEPTION_ */
